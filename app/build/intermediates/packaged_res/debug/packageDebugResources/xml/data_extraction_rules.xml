<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude GPS files from cloud backup for privacy -->
        <exclude domain="file" path="gps/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow GPS files to be transferred to new device -->
        <include domain="file" path="gps/"/>
    </device-transfer>
</data-extraction-rules>
